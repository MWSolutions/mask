{
  "name": "@mwsolutions/mask",
  "version": "1.0.0",
  "description": "Different variable types with a mask",
  "author": "Marc Willemsen",
  "license": "MIT",
  "keywords": [
    "mask"
  ],
  "type": "module",
  "main": "./dist/mask.umd.cjs",
  "module": "./dist/mask.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mask.js",
      "require": "./dist/mask.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run --reporter verbose",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc 'lib/**/*.ts' --write"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.25.3"
  }
}
